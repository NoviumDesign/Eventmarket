include includes/head
body(class="#{locals.pageClass}")
  include includes/nav
  include includes/messages
  .row
    include includes/sidenav

    .large-9.columns.medium-8.columns
      h3 Inspirationsbilder
      .row
        .small-12.columns
          .right
            span.dynatable-custom-select Placering
              select#search-responsible(name='')
                option Alla
                option Startsidan
                option Event
                option Konferens
                option Erbjudande
                option Bildarkivet
                option Backstage
                option Biljett
            span.dynatable-custom-select Synlig/Släckt
              select#search-activity(name='')
                option Alla
                option Synlig
                option Släckt
          span#dynatable-search-group-table.dynatable-search Search:
            input#dynatable-query-search-group-table(type='search', data-dynatable-query='search')
          table#group-table.dynamic-table
            thead
              tr
                th(data-dynatable-column="") Placering
                th(data-dynatable-column="") Rubrik
                th(data-dynatable-column="") Startdatum
                th(data-dynatable-column="") Slutdatum
                th(data-dynatable-column="") Ordning
                th(data-dynatable-column="") Synlig/Släckt
            tbody
              tr
                td
                td
                td
                td
                td
                td
              tr
                td
                td
                td
                td
                td
                td

  include includes/footer